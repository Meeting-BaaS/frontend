{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [
    "AUTH_COOKIE_PREFIX",
    "BETTER_AUTH_SECRET",
    "DATABASE_URL",
    "USER_ENCODING_KEY",
    "BOTS_API_KEY_SECRET",
    "GOOGLE_ID",
    "GOOGLE_SECRET",
    "MICROSOFT_ID",
    "MICROSOFT_SECRET",
    "MICROSOFT_TENANT_ID",
    "ZOOM_ID",
    "ZOOM_SECRET",
    "GITHUB_ID",
    "GITHUB_SECRET",
    "GITLAB_ID",
    "GITLAB_SECRET",
    "DOMAIN",
    "TRUSTED_ORIGINS",
    "NEXT_PUBLIC_ENVIRONMENT",
    "NEXT_PUBLIC_BASE_DOMAIN",
    "API_SERVER_BASEURL",
    "NEXT_PUBLIC_REDIS_URL_MCP",
    "AI_CHAT_DB",
    "AI_CHAT_APP_URL",
    "EMAIL_API_SERVER_BASEURL",
    "EMAIL_SERVICE_API_KEY",
    "NEXT_PUBLIC_SUPPORT_EMAIL",
    "SELF_HOST",
    "NEXT_PUBLIC_AUTH_APP_URL",
    "IMAGE_HOST",
    "STRIPE_SCALE_PRODUCT_ID",
    "STRIPE_ENTERPRISE_PRODUCT_ID",
    "STRIPE_STARTER_PACK_PRODUCT_ID",
    "STRIPE_PRO_PACK_PRODUCT_ID",
    "STRIPE_BUSINESS_PACK_PRODUCT_ID",
    "STRIPE_ENTERPRISE_PACK_PRODUCT_ID",
    "STRIPE_SCALE_PRICE_ID",
    "STRIPE_ENTERPRISE_PRICE_ID",
    "STRIPE_STARTER_PACK_PRICE_ID",
    "STRIPE_PRO_PACK_PRICE_ID",
    "STRIPE_BUSINESS_PACK_PRICE_ID",
    "STRIPE_ENTERPRISE_PACK_PRICE_ID",
    "NEXT_PUBLIC_EMAIL_BATCH_SIZE"
  ]
}
